<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="product_table">

	<!-- pdh: 특정 상품 종류에 따른 상품 전체 리스트 가져오기 -->
	<select id="products_list" resultType="ProductsVO" parameterType="String">
		select * 
		from product_table 
		where p_type = #{p_type}
	</select>
</mapper>















